{
  "private": true,
  "name": "unimed-aio",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "expo start --web --https",
    "apk:release": "yarn jetify && yarn assembleRelease",
    "build:web": "expo build:web --clear --no-pwa",
    "build:web:production": "set EXPO_WEB_DEBUG=false&& set GENERATE_SOURCEMAP=false&& yarn build:web",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "assembleDebug": "yarn bundle:android && cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "assemble:release:android": "cd ./android && ./gradlew app:assembleRelease",
    "assemble:staging:android": "yarn bundle:android && cd ./android && gradlew app:assembleStaging",
    "install:staging:android": "react-native run-android --variant=staging",
    "install:staging:ios": "react-native run-ios --variant=staging",
    "install:release:android": "react-native run-android --variant=release",
    "install:release:ios": "react-native run-ios --variant=release",
    "appcenter:staging:android": "appcenter codepush release-react -a untouchable.rb-gmail.com/unimed-android -d Staging",
    "appcenter:staging:ios": "appcenter codepush release-react -a untouchable.rb-gmail.com/unimed-ios -d Staging",
    "appcenter:production:android": "appcenter codepush release-react -a untouchable.rb-gmail.com/unimed-android -d Production",
    "appcenter:production:ios": "appcenter codepush release-react -a untouchable.rb-gmail.com/unimed-ios -d Production",
    "appcenter:keys:android": "appcenter codepush deployment list --app untouchable.rb-gmail.com/unimed-android -k",
    "appcenter:keys:ios": "appcenter codepush deployment list --app untouchable.rb-gmail.com/unimed-ios -k",
    "deploy:netlify": "netlify deploy --dir=web-build",
    "deploy:ngrok": "ngrok http 19006",
    "upload": "yarn build:web && yarn deploy:netlify"
  },
